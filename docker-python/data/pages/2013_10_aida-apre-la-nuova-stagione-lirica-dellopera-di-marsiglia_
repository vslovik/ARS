ity="48" Name="Grid Table 3 Accent 4"/> <w:LsdException Locked="false" Priority="49" Name="Grid Table 4 Accent 4"/> <w:LsdException Locked="false" Priority="50" Name="Grid Table 5 Dark Accent 4"/> <w:LsdException Locked="false" Priority="51" Name="Grid Table 6 Colorful Accent 4"/> <w:LsdException Locked="false" Priority="52" Name="Grid Table 7 Colorful Accent 4"/> <w:LsdException Locked="false" Priority="46" Name="Grid Table 1 Light Accent 5"/> <w:LsdException Locked="false" Priority="47" Name="Grid Table 2 Accent 5"/> <w:LsdException Locked="false" Priority="48" Name="Grid Table 3 Accent 5"/> <w:LsdException Locked="false" Priority="49" Name="Grid Table 4 Accent 5"/> <w:LsdException Locked="false" Priority="50" Name="Grid Table 5 Dark Accent 5"/> <w:LsdException Locked="false" Priority="51" Name="Grid Table 6 Colorful Accent 5"/> <w:LsdException Locked="false" Priority="52" Name="Grid Table 7 Colorful Accent 5"/> <w:LsdException Locked="false" Priority="46" Name="Grid Table 1 Light Accent 6"/> <w:LsdException Locked="false" Priority="47" Name="Grid Table 2 Accent 6"/> <w:LsdException Locked="false" Priority="48" Name="Grid Table 3 Accent 6"/> <w:LsdException Locked="false" Priority="49" Name="Grid Table 4 Accent 6"/> <w:LsdException Locked="false" Priority="50" Name="Grid Table 5 Dark Accent 6"/> <w:LsdException Locked="false" Priority="51" Name="Grid Table 6 Colorful Accent 6"/> <w:LsdException Locked="false" Priority="52" Name="Grid Table 7 Colorful Accent 6"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 1"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 1"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 1"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 1"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 1"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 1"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 1"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 2"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 2"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 2"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 2"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 2"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 2"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 2"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 3"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 3"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 3"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 3"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 3"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 3"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 3"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 4"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 4"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 4"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 4"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 4"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 4"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 4"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 5"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 5"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 5"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 5"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 5"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 5"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 5"/> <w:LsdException Locked="false" Priority="46" Name="List Table 1 Light Accent 6"/> <w:LsdException Locked="false" Priority="47" Name="List Table 2 Accent 6"/> <w:LsdException Locked="false" Priority="48" Name="List Table 3 Accent 6"/> <w:LsdException Locked="false" Priority="49" Name="List Table 4 Accent 6"/> <w:LsdException Locked="false" Priority="50" Name="List Table 5 Dark Accent 6"/> <w:LsdException Locked="false" Priority="51" Name="List Table 6 Colorful Accent 6"/> <w:LsdException Locked="false" Priority="52" Name="List Table 7 Colorful Accent 6"/> </w:LatentStyles> </xml><![endif]--><!--[if gte mso 10]><style>table.MsoNormalTable{mso-style-name:"Tabella normale";mso-tstyle-rowband-size:0;mso-tstyle-colband-size:0;mso-style-noshow:yes;mso-style-priority:99;mso-style-parent:"";mso-padding-alt:0cm 5.4pt 0cm 5.4pt;mso-para-margin:0cm;mso-para-margin-bottom:.0001pt;mso-pagination:widow-orphan;font-size:10.0pt;font-family:"Times New Roman","serif"}</style><![endif]--><span style="font-size: 12.0pt; font-family: \'Times New Roman\',\'serif\'; mso-fareast-font-family: \'Times New Roman\'; mso-ansi-language: IT; mso-fareast-language: AR-SA; mso-bidi-language: AR-SA;">[fine-scheda] </span><span id="result_box" lang="it"><span class="hps"><a href="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3190-photo-Christian-DRESSE-2013.jpg" rel="prettyphoto[58922]"><img class="alignleft size-medium wp-image-58927" title="IMG_3190 photo Christian DRESSE 2013" alt="" src="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3190-photo-Christian-DRESSE-2013-290x193.jpg" width="290" height="193" srcset="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3190-photo-Christian-DRESSE-2013-290x193.jpg 290w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3190-photo-Christian-DRESSE-2013-1024x682.jpg 1024w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3190-photo-Christian-DRESSE-2013-270x180.jpg 270w" sizes="(max-width: 290px) 100vw, 290px" /></a>Dopo</span> </span><span id="result_box" lang="it"><span class="hps"><em><a href="http://www.gbopera.it/2013/06/marsiglia-opera-municipal-cleopatre/#more-54658">Cloptre</a> </em>di Massenet</span> <span class="hps">e <a href="http://www.gbopera.it/2013/07/marsiglia-opera-municipal-les-troyens/#more-55778"><em>Les Troyens</em></a> di Berlioz che hanno chiuso la precedente stagione</span><span>,</span> nell&#8217;ambito del<span class="hps"> progetto culturale &#8220;Marsiglia</span> <span class="hps">e il Mediterraneo</span>&#8220;, l&#8217;Opra apre nel segno del <span class="hps">200  anniversario</span> <span class="hps">della nascita di</span> <span class="hps">Giuseppe Verdi</span> con la popolarissima <em>Aida</em><span>.</span><br /> </span><span id="result_box" lang="it"><span class="hps">Una produzione del capolavoro verdiano non nuova, ma del 2008, gi salutata allora da un successo convinto riconfermato da questa ripresa. Il regista <strong>Charles Roubaud</strong>, con il supporto dell&#8217;impianto scenico di <strong>Emmanuelle Fabre, </strong>degli eleganti costumi di <strong>Katia Duflot</strong> e le luci di <strong>Philippe Grosperin </strong>ci hanno offerto uno spettacolo tradizionale, ma nel contempo moderno, per la sobriet visiva che si allontana dai clich visivi legati a questo titolo. L&#8217;Egitto dei Faraoni  presente ma attraverso un efficace uso di proizioni in 3D che, aiutate da un uso efficace delle luci, creano delle atmosfere teatralmente efficaci e coinvolgenti. Tutto  comunque fortemente stilizzato, giocato su simbologie, cos come la regia che guarda agli aspetti pi intimi dell&#8217;opera. Le ancenstrali e tribali coreografie di <strong>Laurence Fanon </strong>si inseriscono perfettamente in questa visione di <em>Aida.<br /> </em><a href="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3353-photo-Christian-DRESSE-2013.jpg" rel="prettyphoto[58922]"><img class="alignleft size-medium wp-image-58925" title="IMG_3353 photo Christian DRESSE 2013" alt="" src="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3353-photo-Christian-DRESSE-2013-133x200.jpg" width="133" height="200" srcset="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3353-photo-Christian-DRESSE-2013-133x200.jpg 133w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3353-photo-Christian-DRESSE-2013-511x768.jpg 511w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3353-photo-Christian-DRESSE-2013-270x405.jpg 270w" sizes="(max-width: 133px) 100vw, 133px" /></a>Il soprano canadese </span></span><span id="result_box" lang="it"><span class="hps"><strong>Michle Capalbo</strong> (chiamata a sostituire la prevista Elena Pankratova) debuttava a Marsiglia. Dotata di un indubbio fascino scenico, ci ha presentato un&#8217;Aida tutt&#8217;altro che remissiva (il costume stesso ci  parso un po&#8217; troppo regale). Anche il canto sembra inzialmente prendere la via di un&#8217;Aida a &#8220;tinte forti&#8221; (il suo &#8220;Ritorna vincitor&#8221;  stato troppo insistito nei &#8220;forti&#8221;), fortunatamente per, nel corso dell&#8217;opera, la Capalbo, oltre a un&#8217;inontanazione perfetta e a uno strumento omogeneo nell&#8217;emissione e ben proiettato, ha mostrato una sempre maggiore dolcezza, duttilit di colori ed espressivat. Accanto a Zoran Todorovich ha interpretato magistralmente il duetto finale dell&#8217;opera. Accanto a lei, <strong>Sonia Ganassi</strong> che debuttava il ruolo di Amneris. Il canto della Ganassi  sicuro nell&#8217;intera linea vocale. La sua formazione belcantista si percepisce nella musicalit, nella dizione accurata, nell&#8217;approccio al personaggio sempre emotivamente presente ma mai sopra le righe. <a href="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3346-photo-Christian-DRESSE-2013.jpg" rel="prettyphoto[58922]"><img class="alignleft size-medium wp-image-58924" title="IMG_3346 photo Christian DRESSE 2013" alt="" src="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3346-photo-Christian-DRESSE-2013-290x193.jpg" width="290" height="193" srcset="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3346-photo-Christian-DRESSE-2013-290x193.jpg 290w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3346-photo-Christian-DRESSE-2013-1024x682.jpg 1024w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3346-photo-Christian-DRESSE-2013-270x180.jpg 270w" sizes="(max-width: 290px) 100vw, 290px" /></a>Anche<em></em></span></span><span id="result_box" lang="it"><span class="hps"><strong>Zoran Todorovich</strong> debuttava come Radames e, nonostante l&#8217;annnuncio di una indisposizione, il cantante  venuto a capo del tutt&#8217;altro che facile ruolo con sicurezza e convinzione. La voce  calda nel timbro, controllata nella tessitura. Todorovich ha inoltre mostrato un buona ricerca espressiva volta a creare non solo il guerriero ma anche l&#8217;uomo innamorato. </span></span><strong>Marco di Felice</strong> completa il quartetto dei protagonisti. Questo cantante si era gi fatto apprezzare nel 2010 come Grard nell&#8217;<em> Andra </em><em>Chnier</em> anche qui accanto a Zoran Todorovic. L&#8217;interprete appare un po&#8217; convenzionale nei gesti, ma il canto di Di Felice  perfettamente in linea con il personaggio verdiano. <a href="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3791-photo-Christian-DRESSE-2013.jpg" rel="prettyphoto[58922]"><img class="alignleft size-medium wp-image-58932" title="IMG_3791 photo Christian DRESSE 2013" alt="" src="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3791-photo-Christian-DRESSE-2013-290x193.jpg" width="290" height="193" srcset="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3791-photo-Christian-DRESSE-2013-290x193.jpg 290w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3791-photo-Christian-DRESSE-2013-1024x682.jpg 1024w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3791-photo-Christian-DRESSE-2013-270x180.jpg 270w" sizes="(max-width: 290px) 100vw, 290px" /></a>Abbiamo ascoltato un Amonasro dall&#8217;accento indomabile e dal canto pieno e vibrante.<br /> Il resto del cast si avvale delle presenza di <strong>Luiz-Ottavio Faria</strong> un Ramfis di buona statura. Superata un&#8217;iniziale incertezza, Faria si  via, via riscaldato arrivando alla &#8220;scena del giudizio&#8221; dell&#8217;atto IV con una voce pienamente sonora e ampia. Buono, anche se piuttosto disomogeno nell&#8217;emissione, il basso <strong>Philippe Kahn</strong> (il Re). Seppur nei loro brevi interventi, abbiamo comunque apprezzato le qualit vocali e la musicalit di <strong>Ludivine Gombert </strong>(Sacerdotessa) e di <strong>Wilfried Tissot </strong>(il messaggero). Il Coro dell&#8217;Opra, preparato da <strong>Pierre Iodice</strong> ha dato prova di compattezza e duttilit espressiva.<br /> <a href="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_5031-photo-Christian-DRESSE-2013.jpg" rel="prettyphoto[58922]"><img class="alignleft size-medium wp-image-58935" title="IMG_5031 photo Christian DRESSE 2013" alt="" src="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_5031-photo-Christian-DRESSE-2013-290x193.jpg" width="290" height="193" srcset="http://www.gbopera.it/wp-content/uploads/2013/10/IMG_5031-photo-Christian-DRESSE-2013-290x193.jpg 290w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_5031-photo-Christian-DRESSE-2013-1024x682.jpg 1024w, http://www.gbopera.it/wp-content/uploads/2013/10/IMG_5031-photo-Christian-DRESSE-2013-270x180.jpg 270w" sizes="(max-width: 290px) 100vw, 290px" /></a>Concludiamo con la concertazione, affidata a <strong>Fabrizio Maria Carminati</strong>, primo direttore ospite de l&#8217;Opra de Marseille. La sua direzione si  caratterizzata per una dinamica ricca e varia, un sicuro controllo tra buca e palco, un lodevole senso delle rifiniture e, soprattutto, la capacit di creare le giuste atmosfere. L&#8217;orchestra ha risposto positivamente e con precisione, alle intentezioni interpretative di Carminati. Una menzione speciale alle sonore e intonatissime trombe della marcia trionfale. Una rappresentazione di <em>Aida </em>salutata da un pieno e convincente successo di pubblico.<em> Foto Christian Dresse</em></p> <a class="wpptopdf" target="_blank" rel="noindex,nofollow" href="http://www.gbopera.it/2013/10/aida-apre-la-nuova-stagione-lirica-dellopera-di-marsiglia/?format=pdf" title="Download PDF"><img alt="Download PDF" src="http://www.gbopera.it/wp-content/plugins/wp-post-to-pdf/asset/images/pdf.png"></a></div><div class="clear"></div><div class="entry-extra clearfix"><div class="comments-link " style="color: #363636"> <i class="icon-comment"></i> <a href="http://www.gbopera.it/2013/10/aida-apre-la-nuova-stagione-lirica-dellopera-di-marsiglia/#respond"><span class="leave-reply" style="color:#363636 !important;">Leave a reply</span></a></div><div class="entry-share"> <span style="color: #363636">Share This<i class="icon-plus"></i></span><div class="entry-share-icons"> <span class="ct-pinterest" title="Share on Pinterest"><a href="http://pinterest.com/pin/create/button/?url=http://www.gbopera.it/2013/10/aida-apre-la-nuova-stagione-lirica-dellopera-di-marsiglia/&amp;media=http://www.gbopera.it/wp-content/uploads/2013/10/IMG_3639